    "debug": "^2.2.0",
    "keef": "^1.1.1",
    "path-to-regexp": "^1.7.0",
    "request": "^2.76.0",
    "ringpop": "^10.16.2",
    "seeli": "^3.0.1",
    "ioredis": "^2.4.0",
    "tchannel": "^3.9.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "mocha": "^3.2.0",
    "supertest": "^2.0.1"
